{
  "name": "meli-challenge",
  "version": "1.0.0",
  "description": "Challenge para MELI",
  "private": true,
  "scripts": {
    "dev:client": "webpack serve --config webpack.client.js --mode development",
    "dev:server:build": "webpack watch --config webpack.server.js --mode development",
    "dev:server:run": "nodemon dist/index.js",
    "dev": "concurrently npm:dev:*",
    "build:client": "webpack build --config webpack.client.js --mode production",
    "build:server": "webpack build --config webpack.server.js --mode production",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json}'",
    "test": "jest",
    "prepare": "husky install",
    "release": "release-it --config .release-it.json"
  },
  "author": "Ian Mancini <contacto@ianmancini.com.ar>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ametis70/meli-challenge.git"
  },
  "bugs": {
    "url": "https://github.com/ametis70/meli-challenge/issues"
  },
  "homepage": "https://github.com/ametis70/meli-challenge#readme",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@release-it/conventional-changelog": "^3.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet-async": "^1.0.3",
    "auto-changelog": "^2.3.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.5.1",
    "copy-webpack-plugin": "^10.2.0",
    "cors": "^2.8.5",
    "css-loader": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "release-it": "^14.11.8",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
